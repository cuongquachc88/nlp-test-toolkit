# Server Configuration
NODE_ENV=development
PORT=3000

# Database
DATABASE_PATH=./data/llmtestkit.db

# LLM Providers Configuration
PRIMARY_LLM_PROVIDER=openai
FALLBACK_LLM_PROVIDERS=gemini,glm

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.3
OPENAI_MAX_TOKENS=2000

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro
GEMINI_TEMPERATURE=0.2

# ChatGLM (Zhipu AI)
GLM_API_KEY=your_glm_api_key_here
GLM_MODEL=chatglm3-6b
GLM_ENDPOINT=https://open.bigmodel.cn/api/paas/v4

# OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=anthropic/claude-3-opus

# AgentRouter
AGENTROUTER_API_KEY=your_agentrouter_api_key_here
AGENTROUTER_ENDPOINT=https://agentrouter.org/v1
AGENTROUTER_STRATEGY=balanced

# Browser Configuration
HEADLESS=true
BROWSER_TIMEOUT=30000

# Logging
LOG_LEVEL=info
